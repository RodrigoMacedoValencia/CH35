package bucles;

public class While {
	
	public static void main(String[] args) {
		
		/*While ejecuta un bloque de c贸digo hasta que se cumpla la condici贸n dada.
		 * 
		 * while (expresi贸n)
		 * bloque de c贸digo a ejecutar
		 * (contador-iterador)
		 * j
		*/
		int cuenta = 0;
		while (cuenta < 10) {
			cuenta ++;
			System.out.println("la cuenta es de "+cuenta);
			
		}
		
	}
}
